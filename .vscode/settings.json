{
  "editor.formatOnSave": true,
  "editor.formatOnSaveTimeout": 1500,
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~#$%^&*()-=+[{]}\\|;:'\",.<>/",
  "files.associations": {
    "*.sinew": "ruby"
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "ruby.format": "rubocop",
  "ruby.lint": {
    "rubocop": true
  }
}
